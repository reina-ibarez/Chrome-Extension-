{
  "manifest_version": 2,

  "name": " UNICORNS  ",
  "description": "AMAZING",
  "version": "1.0",
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["jquery-3.2.1.min.js", "content.js"]
        }
    ],
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ]
}

